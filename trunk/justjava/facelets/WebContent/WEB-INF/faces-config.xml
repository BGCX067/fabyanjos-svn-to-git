<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <managed-bean>
  <managed-bean-name>autenticador</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.acesso.Acesso</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroUsuario</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.acesso.Acesso</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroDados</managed-bean-name>
  <managed-bean-class>br.com.grecco.dados.Dados</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dadosHandler</managed-bean-name>
  <managed-bean-class>br.com.grecco.handler.dadoshandler.DadosHandler</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroCliente</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.cliente.CadastroCliente</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>clienteHandler</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.clientehandler.ClienteHandler</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroCampanha</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.campanha.CadastroCampanha</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>campanhaHandler</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.campanhahandler.CampanhaHandler</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroVisita</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.visita.CadastroVisita</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>visitaHandler</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.visitahandler.VisitaHandler</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>acaoCadastroRM</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.rm.CadastroRM</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>rmHandler</managed-bean-name>
  <managed-bean-class>br.com.justjava.sgv.cadastro.rmhandler.RMHandler</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pages/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>usuario</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>menuCadastroCampanha</from-outcome>
   <to-view-id>/pages/cadastroCampanha.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuCadastroCliente</from-outcome>
   <to-view-id>/pages/cadastroCliente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuCadastroRM</from-outcome>
   <to-view-id>/pages/cadastroRM.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuCadastroVisita</from-outcome>
   <to-view-id>/pages/cadastroVisita.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuHome</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <message-bundle>messages</message-bundle>
  <locale-config>
   <default-locale>pt_BR</default-locale>
  </locale-config>
 </application>
</faces-config>
